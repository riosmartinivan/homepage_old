{
  "name": "homepage",
  "author": "Phorus Group",
  "description": "Phorus Group homepage",
  "license": "GPL-3.0-only",
  "engines": {
    "npm": "please-use-yarn"
  },
  "resolutions": {
    "webpack": "^5"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "eslint **/*.test.ts* **/*.ts* --fix --no-error-on-unmatched-pattern",
    "storybook": "sb dev",
    "storybook:build": "sb build",
    "test:unit": "jest --ci --json --outputFile=test/.jest-test-results.json",
    "test:integration": "yarn storybook && test-storybook",
    "test:cucumber": "cucumber-js -p default"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn lint"
    ]
  },
  "jest-junit": {
    "outputDirectory": "./test/.coverage",
    "outputName": "junit.xml"
  },
